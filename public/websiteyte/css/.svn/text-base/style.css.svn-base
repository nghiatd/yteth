}
.body-wrapper{
	width:100%;
	margin:0;
	padding:0;
	float:left;
	z-index:9999;
	position:relative;
	height:auto;
	min-height:100%;
}
.controller{
	width:1000px;
	margin:0 auto;
	min-height:100%;
}
.controller2{
	width:100%;
	float:left;
	background:#FFF;
