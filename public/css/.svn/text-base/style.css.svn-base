/****************************style css via toolbaxnhansu******************************/
.nav-menu-top > ul {
  margin-top: !important;
}
.nav-menu-top ul {
  height: 32px;
  list-style: none;
  margin: 0;
  padding: 0;
}
.nav-menu-top ul ul {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.nav-menu-top ul ul a {
}
.nav-menu-top ul ul ul {
  left: 100%;
  top: 0;
}
.nav-menu-top li {
  float: left;
  padding: 0px 8px 0px 8px;
}
.nav-menu-top li a {
  display: block;
  font-weight: bold;
  line-height: 30px;
  padding: 0px 13px;
  text-align: center;
  text-decoration: none;
}
.nav-menu-top li a:hover {
  	border:1px solid #f7d74a;
	background-image: linear-gradient(bottom, rgb(255,247,206) 37%, rgb(255,226,140) 69%);
	background-image: -o-linear-gradient(bottom, rgb(255,247,206) 37%, rgb(255,226,140) 69%);
	background-image: -moz-linear-gradient(bottom, rgb(255,247,206) 37%, rgb(255,226,140) 69%);
	background-image: -webkit-linear-gradient(bottom, rgb(255,247,206) 37%, rgb(255,226,140) 69%);
	background-image: -ms-linear-gradient(bottom, rgb(255,247,206) 37%, rgb(255,226,140) 69%);
}
.nav-menu-top li ul {
  background:#f0f0f0;
  display: none;
  height: auto;
  filter: alpha(opacity=95);
  opacity: 0.95;
  position: absolute;
  width: 205px;
  z-index: 200;
  border-left: 1px solid gray;
  border-right: 1px solid gray;
  border-bottom: 1px solid gray;
  /*top:1em;
		/*left:0;*/

}
.nav-menu-top li:hover > ul {
  display: block;
}
.nav-menu-top li li {
  display: block;
  padding: 0px;
  position: relative;
  width: 205px;
}
.nav-menu-top li ul a {
  display: block;
  font-size: 12px;
  font-style: normal;
  font-weight:normal;
  padding: 0px 30px;
  text-align: left;
  line-height:2em;
}
.nav-menu-top li ul a:hover {
	background:#fafafa;
    border:1px solid #7eabcd;
	border-radius:3px;
}
.nav-menu-top p {
  clear: left;
}
.nav-menu-top .active > a {
}
.nav-menu-top .active > a:hover {
  color: black;
}
.nav-menu-top li ul li ul{
	width:400px;
}
.nav-menu-top li ul li ul li{
	width:400px;
}

