				$data ['NhanvienId'] = $item ['Id'];
				$data ['ThongtincoquanId'] = $this->_ThongtincoquanId;
				$data ['Ngaythang'] =$ngaynghi;
				$data ['Lydo'] =  $item ['Soca'];
