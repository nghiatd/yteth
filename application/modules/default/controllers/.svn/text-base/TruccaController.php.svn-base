	//cap nhat du lieu
	public function updateAction(){
		$id = $this->_getParam('id');
		$this->_helper->layout()->disableLayout();
		$jsonObj = array();
		$Ngaythang = $this->convertDate($_REQUEST['Ngaythang']);
				$data = array ();
